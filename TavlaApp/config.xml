<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="no.tavla.tavlaapp" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Tavla</name>
  <description>Keeps everything in place</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Lars Erik Finholt</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Fullscreen" value="true" />
  <vs:plugin name="com.msopentech.azure-mobile-services" version="0.1.4" />
  <vs:plugin name="nl.x-services.plugins.calendar" version="4.2.9" />
  <vs:platformSpecificValues />
</widget>